<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE niftoolsxml>
<niftoolsxml version="0.7.1.0">

	<xi:include href="../ovl_base/ovl_base.xml" xmlns:xi="http://www.w3.org/2001/XInclude" xpointer="xpointer(*/*)" />

    <compound name="SemanticFlexiColoursRoot" inherit="MemStruct">
        <add name="name_list"  type="ArrayPointer" template="colourname" arg="name_count"/>
        <add name="name_count"  type="uint64" />
    </compound>

    <compound name="colourname" inherit="MemStruct">
        <add name="colour_name"  type="Pointer" template="ZString"/>
    </compound>



</niftoolsxml>
