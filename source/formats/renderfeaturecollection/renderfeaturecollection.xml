<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE fileformat>
<fileformat>

	<xi:include href="../ovl_base/ovl_base.xml" xmlns:xi="http://www.w3.org/2001/XInclude" xpointer="xpointer(*/*)" />

    # enough to process all the bytes, just no idea yet what is each section.
 
    <struct name="RenderFeatureCollectionRoot" inherit="MemStruct">
        <field name="item_list"   type="ArrayPointer" template="RenderFeatureItem" arg="item_count"/>
        <field name="item_count"  type="uint64" />
    </struct>

    # 24 bytes long
    <struct name="RenderFeatureItem" inherit="MemStruct">
        <field name="item_name"  type="Pointer" template="ZString"/>
        <field name="item_data"   type="ArrayPointer" template="RenderFeatureSubItem" arg="item_data_count"/>
        <field name="item_data_count"  type="uint64" />
    </struct>

    <struct name="RenderFeatureSubItem" inherit="MemStruct">
        <field name="sub_item_name"  type="Pointer" template="ZString"/>
        <field name="sub_item_value_or_flags" type="uint64" />
    </struct>


</fileformat>
