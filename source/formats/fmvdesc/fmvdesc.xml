        # found 65 all the time
        <field name="unk_65"    type="uint" />
        # found 1.0 in speed all the time, so just a guess
        <field name="speed"      type="float" />
        <field name="_zero03"    type="uint64" />
        <field name="_zero04"    type="uint64" />
        <field name="_zero05"    type="uint64" />
        <field name="_zero06"    type="uint64" />
    </struct>

</fileformat>
